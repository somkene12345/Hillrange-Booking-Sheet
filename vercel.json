{
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
 "routes": [
    { "src": "/explore", "dest": "/dist/explore.html" },
    { "src": "/home", "dest": "/dist/index.html" },
    { "src": "/#", "dest": "/dist/index.html" },
    { "src": "/(.*)", "dest": "/dist/$1" }
  ]
}
